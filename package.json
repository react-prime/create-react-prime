    "kleur": "4.1.4",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "7.3.3",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.6",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "esbuild": "0.13.9",
    "esbuild-runner": "2.2.1",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
