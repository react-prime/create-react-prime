{
  "name": "create-react-prime",
  "version": "2.0.0-beta.11",
  "description": "Simplifies setting up your next React-prime project by automating the entire process.",
  "author": "Label A [labela.nl]",
  "license": "MIT",
  "main": "dist/main",
  "bin": {
    "create-react-prime": "dist/main"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig.app.json && npm run build:env && npm run build:cp",
    "build:cp": "cp src/main dist;cp ./{tsconfig-paths-bootstrap.js,tsconfig.json} dist/",
    "build:env": "node envify.js",
    "start": "node dist/main",
    "dev": "rm -rf testingname/ testing-name/; npm run build; node dist/main testing-name",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "npm t -- --watch",
    "test:full": "npm run lint; npm run typecheck; npm t",
    "prepublishOnly": "npm run test:full && npm run build",
    "pub:patch": "npm version patch; npm publish",
    "pub:minor": "npm version minor; npm publish",
    "pub:major": "npm version major; npm publish"
  },
  "dependencies": {
    "commander": "5.1.0",
    "inquirer": "7.3.0",
    "inversify": "5.0.1",
    "kleur": "4.0.2",
    "ora": "4.0.4",
    "reflect-metadata": "0.1.13",
    "tsconfig-paths": "3.9.0"
  },
  "devDependencies": {
    "@types/inquirer": "6.5.0",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.13",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-plugin-import": "2.21.2",
    "jest": "26.1.0",
    "loose-envify": "1.4.0",
    "ts-jest": "26.1.1",
    "typescript": "3.9.5",
    "typescript-eslint-parser": "22.0.0"
  },
  "keywords": [
    "boilerplate",
    "install",
    "cli",
    "react",
    "react-native",
    "nextjs",
    "react-prime"
  ]
}
